  "bugs": {
    "url": "https://github.com/mattmatt0/Almati-V2/issues"
  },
  "homepage": "https://github.com/mattmatt0/Almati-V2#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.3.1",
    "mariadb": "^2.5.2",
    "ts-mixer": "^5.4.0"
  }
